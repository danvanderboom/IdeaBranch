<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="IdeaBranch.App.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:IdeaBranch.App"
    xmlns:views="clr-namespace:IdeaBranch.App.Views"
    Title="IdeaBranch.App">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    
    <ShellContent
        Title="Resilience Test"
        ContentTemplate="{DataTemplate views:ResilienceTestPage}"
        Route="ResilienceTestPage" />

</Shell>
