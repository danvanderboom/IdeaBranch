<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:vm="clr-namespace:IdeaBranch.App.ViewModels"
             xmlns:res="clr-namespace:IdeaBranch.App.Resources"
             x:Class="IdeaBranch.App.Views.MapPage"
             x:DataType="vm:MapViewModel"
             Title="{x:Static res:AppResources.MapPage_Title}"
             AutomationId="MapPage">
    <Grid>
        <Label Text="{x:Static res:AppResources.MapPage_Content}"
               FontSize="24"
               HorizontalOptions="Center"
               VerticalOptions="Center"
               AutomationId="MapPage_Content" />
    </Grid>
</ContentPage>
