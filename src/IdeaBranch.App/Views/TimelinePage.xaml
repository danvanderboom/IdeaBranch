<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:vm="clr-namespace:IdeaBranch.App.ViewModels"
             xmlns:res="clr-namespace:IdeaBranch.App.Resources"
             x:Class="IdeaBranch.App.Views.TimelinePage"
             x:DataType="vm:TimelineViewModel"
             Title="{x:Static res:AppResources.TimelinePage_Title}"
             AutomationId="TimelinePage">
    <Grid>
        <Label Text="{x:Static res:AppResources.TimelinePage_Content}"
               FontSize="24"
               HorizontalOptions="Center"
               VerticalOptions="Center"
               AutomationId="TimelinePage_Content" />
    </Grid>
</ContentPage>
